<% include partials/header %>
<div class="content-wrapper">
  <div class="row col-md-14">
    <div class="box box-info col-md-12">
      <div class="box-header with-border" style="
      height: 10vh;
      display: flex;
      align-items: center;
      justify-content: center;
  ">
        <h3 class="box-title" style="
        font-size: xx-large;
    ">Updation Form</h3>
      </div>
      <div class="box-header with-border" id="formselect" style="
      height: 10vh;
      display: flex;
      align-items: center;
      justify-content: center;
  ">
        click on any one:::
        <span id="landreg" value="Land Updation">Land Updation</span>/
        <span id="personreg" value="Person Updation">Person Updation</span> <br>

        <script>
          $("#formselect").hover(function () {
            $(this).css("cursor", "pointer")
          });
          $("#landreg").click(function () {
            $("#landform").css("display", "block");
            $("#personform").css("display", "none");

          });
          $("#personreg").click(function () {
            $("#personform").css("display", "block");
            $("#landform").css("display", "none");
          });
        </script>
      </div>

      <form action="/peer/land_updation" id="landform" method="POST" class="form-horizontal" style="
    display: inline-block;
    justify-content: center;
    align-items: center;
    display: none;
">
        <div class="box-body col-md-5">
          <div class="form-group">
            <label for="inputName3" class="col-sm-3 control-label" style="text-align:start">Address</label>

            <div class="col-sm-9">
              <input type="text" name="address" class="form-control" placeholder="Address">
            </div>
          </div>
          <!-- yaha par -->
          <div class="form-group">
            <label for="inputPassword3" class="col-sm-3 control-label" style="text-align:start">State</label>

            <div class="col-sm-8">
              <input type="text" class="form-control" id="inputPassword3" name="state" placeholder="State">
            </div>
          </div>
          <div class="form-group">
            <label for="inputName3" class="col-sm-3 control-label" style="text-align:start">City</label>

            <div class="col-sm-8">
              <input type="text" class="form-control" name="city" placeholder="City">
            </div>
          </div>
          <div class="form-group">
            <label for="inputName3" class="col-sm-3 control-label" style="text-align:start">Town Locality</label>

            <div class="col-sm-8">
              <input type="text" class="form-control" name="locality" placeholder="Locality">
            </div>
          </div>
          <div class="form-group">
            <label for="inputName3" class="col-sm-3 control-label" style="text-align:start">Post Office</label>

            <div class="col-sm-8">
              <input type="text" class="form-control" name="postOffice" placeholder="Post Office">
            </div>
          </div>
          <div class="form-group">
            <label for="inputName3" class="col-sm-3 control-label" style="text-align:start">District</label>

            <div class="col-sm-8">
              <input type="text" class="form-control" name="district" placeholder="District">
            </div>

          </div>
          <div class="form-group">
            <label for="inputName3" class="col-sm-3 control-label" style="text-align:start">pincode</label>

            <div class="col-sm-5">
              <input type="text" class="form-control" name="pincode" placeholder="Pincode">
            </div>

          </div>

        </div>

        <div class="box-body col-md-5">
          <div class="form-group">
            <label for="inputName3" class="col-sm-3 control-label" style="text-align:start">road Width</label>

            <div class="col-sm-8">
              <input type="text" class="form-control" name="roadWidth" placeholder="Road Width">
            </div>

          </div>
          <div class="form-group">
            <label for="inputName3" class="col-sm-3 control-label" style="text-align:start">land Width</label>

            <div class="col-sm-8">
              <input type="text" class="form-control" name="landWidth" placeholder="Land Width">
            </div>

          </div>
          <div class="form-group">
            <label for="inputName3" class="col-sm-3 control-label" style="text-align:start">land Height</label>

            <div class="col-sm-8">
              <input type="text" class="form-control" name="landHeight" placeholder="Land Height">
            </div>

          </div>
          <div class="form-group">
            <label for="inputName3" class="col-sm-3 control-label" style="text-align:start">land type</label>

            <div class="col-sm-8">
              <input type="text" class="form-control" name="type" placeholder="Land Type">
            </div>

          </div>
          <div class="form-group">
            <label for="inputName3" class="col-sm-3 control-label" style="text-align:start">gps Coordinates</label>

            <div class="col-sm-4">
              <input type="text" class="form-control" name="lat" placeholder="Latitude">
            </div>
            <div class="col-sm-4">
              <input type="text" class="form-control" name="lng" placeholder="Longitude">
            </div>
          </div>
          <!-- <div class="form-group">
            <label for="inputName3" class="col-sm-3 control-label" style="text-align:start">Property Value</label>

            <div class="col-sm-7" style="
            float: right;
        ">
              <input type="text" class="form-control" id="inputEmail3" placeholder="Property Value">
            </div>
          </div> -->
          <style>
            .col-sm-8,
            .col-sm-5,
            .col-sm-9,
            .col-sm-4 {
              float: right;
            }
          </style>
        </div>
        <div class="clearfix" style="
    height: auto;
    min-height: 0vh;
    display: flex;
    width: 100vw;
"></div>
        <div class="box-footer bg-info" style="
        display: inline-flex;
        align-items: center;
        justify-content: center;
        width: -webkit-fill-available; ">
          <button type="submit" class="btn btn-info">Update</button>
        </div>
        <style>
          .form-group {
            margin-bottom: 25px;
          }
        </style>
      </form>
      <form action="/peer/person_updation" id="personform" method="POST" class="form-horizontal" style="
    display: inline-block;
    justify-content: center;
    align-items: center;
    display: none;
">
        <div class="box-body col-md-6 resize">
          <div class="form-group">
            <label for="inputName3" class="col-sm-3 control-label" style="text-align:start">user id</label>

            <div class="col-sm-9">
              <input type="text" name="userid" class="form-control" placeholder="User ID">
            </div>
          </div>
          <div class="form-group">
            <label for="inputName3" class="col-sm-3 control-label" style="text-align:start">Name</label>

            <div class="col-sm-9">
              <input type="text" name="name" class="form-control" placeholder="Name">
            </div>
          </div>
          <div class="form-group">
            <label for="inputName3" class="col-sm-3 control-label" style="text-align:start">DOB</label>

            <div class="col-sm-9">
              <input type="text" name="dob" class="form-control" placeholder="Date of Birth">
            </div>
          </div>
          <div class="form-group">
            <label for="inputName3" class="col-sm-3 control-label" style="text-align:start">Aadhaar No.</label>

            <div class="col-sm-9">
              <input type="text" name="aadhaarNo" class="form-control" placeholder="Aadhaar No.">
            </div>
          </div>
          <div class="form-group">
            <label for="inputName3" class="col-sm-3 control-label" style="text-align:start">Status</label>

            <div class="col-sm-9">
              <input type="text" name="status" class="form-control" placeholder="Status">
            </div>
          </div>
          <div class="form-group">
            <label for="inputName3" class="col-sm-3 control-label" style="text-align:start">Data</label>

            <div class="col-sm-9">
              <input type="text" name="data" class="form-control" placeholder="Data">
            </div>
          </div>
          <div class="form-group">
            <label for="inputName3" class="col-sm-3 control-label" style="text-align:start">ownership</label>

            <div class="col-sm-9">
              <input type="text" name="ownership" class="form-control" placeholder="Ownership">
            </div>
          </div>

        </div>
        <style>
          .col-sm-8,
          .col-sm-5,
          .col-sm-9,
          .col-sm-4 {
            float: right;
          }
        </style>
        <div class="clearfix" style="
    height: auto;
    min-height: 0vh;
    display: flex;
    width: 100vw;
"></div>
        <div class="box-footer bg-info col-md-6" style="
        display: inline-flex;
        align-items: center;
        justify-content: center; ">
          <button type="submit" class="btn btn-info" style="
    width: -webkit-fill-available;
">Update</button>
        </div>
        <style>
          .form-group {
            margin-bottom: 25px;
          }
        </style>
      </form>
      <style>
        @media only screen and (max-width: 992px) {
          .resize {
            width: 50vw;
          }
        }

        @media only screen and (max-width: 767px) {
          .resize {
            width: 70vw;
          }

          .form-control {
            width: 50vw
          }
        }
      </style>
    </div>
  </div>
</div>

<% include partials/footer %>



















